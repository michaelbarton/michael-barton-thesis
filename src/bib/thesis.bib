	issn = {0028-0836},
	journal = {Nature},
	keywords = {2008-1-mdb, 2008-2-mdb, amino-acid-usage, atomic-content, mdb-thesis, nutrient-limitation},
	month = {September},
	number = {6239},
	pages = {245--248},
	posted-at = {2008-06-17 10:20:34},
	priority = {0},
	title = {Adaptive eradication of methionine and cysteine from cyanobacterial light-harvesting proteins.},
	url = {http://dx.doi.org/10.1038/341245a0},
