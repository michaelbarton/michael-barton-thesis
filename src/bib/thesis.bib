	keywords = {ecoli, evolution, evolutionary-systems-biology, genome-scale-models, genomics, mdb-thesis, metabolic-engineering, metabolomics, network-analysis, phenotype-prediction, reviewed, strain-design, systems-biology},
	month = {June},
	number = {6},
	pages = {659--667},
	posted-at = {2008-06-14 10:35:25},
	priority = {0},
	publisher = {Nature Publishing Group},
	title = {The growing scope of applications of genome-scale metabolic reconstructions using Escherichia coli},
	url = {http://dx.doi.org/10.1038/nbt1401},
	volume = {26},
	year = {2008}
}



@article{heizer2006,
	abstract = {For most prokaryotic organisms, amino acid biosynthesis represents a significant portion of their overall energy budget. The difference in the cost of synthesis between amino acids can be striking, differing by as much as 7-fold. Two prokaryotic organisms, Escherichia coli and Bacillus subtilis, have been shown to preferentially utilize less costly amino acids in highly expressed genes, indicating that parsimony in amino acid selection may confer a selective advantage for prokaryotes. This study confirms those findings and extends them to 4 additional prokaryotic organisms: Chlamydia trachomatis, Chlamydophila pneumoniae AR39, Synechocystis sp. PCC 6803, and Thermus thermophilus HB27. Adherence to codon-usage biases for each of these 6 organisms is inversely correlated with a coding region's average amino acid biosynthetic cost in a fashion that is independent of chemoheterotrophic, photoautotrophic, or thermophilic lifestyle. The obligate parasites C. trachomatis and C. pneumoniae AR39 are incapable of synthesizing many of the 20 common amino acids. Removing auxotrophic amino acids from consideration in these organisms does not alter the overall trend of preferential use of energetically inexpensive amino acids in highly expressed genes. 10.1093/molbev/msl029},
	author = {Heizer, Esley  M.  and Raiford, Douglas  W.  and Raymer, Michael  L.  and Doom, Travis  E.  and Miller, Robert  V.  and Krane, Dan  E. },
	citeulike-article-id = {1236432},
	doi = {http://dx.doi.org/10.1093/molbev/msl029},
	journal = {Mol Biol Evol},
	keywords = {2008-1-mdb, amino-acid-usage, cost-minimisation, gene-characteristics, mdb-thesis},
	month = {September},
	number = {9},
	pages = {1670--1680},
	posted-at = {2009-02-03 15:33:06},
	priority = {2},
	title = {Amino Acid Cost and Codon-Usage Biases in 6 Prokaryotic Genomes: A Whole-Genome Analysis},
	url = {http://dx.doi.org/10.1093/molbev/msl029},
	volume = {23},
	year = {2006}
}



@article{teusink2000,
	abstract = {This paper examines whether the in vivo behavior of yeast glycolysis can be understood in terms of the in vitro kinetic properties of the constituent enzymes. In nongrowing, anaerobic, compressed Saccharomyces cerevisiae the values of the kinetic parameters of most glycolytic enzymes were determined. For the other enzymes appropriate literature values were collected. By inserting these values into a kinetic model for glycolysis, fluxes and metabolites were calculated. Under the same conditions fluxes and metabolite levels were measured. In our first model, branch reactions were ignored. This model failed to reach the stable steady state that was observed in the experimental flux measurements. Introduction of branches towards trehalose, glycogen, glycerol and succinate did allow such a steady state. The predictions of this branched model were compared with the empirical behavior. Half of the enzymes matched their predicted flux in vivo within a factor of 2. For the other enzymes it was calculated what deviation between in vivo and in vitro kinetic characteristics could explain the discrepancy between in vitro rate and in vivo flux.},
	address = {E.C. Slater Institute, BioCentrum Amsterdam, University of Amsterdam, the Netherlands.},
