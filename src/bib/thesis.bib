	keywords = {diauxic-shift, gene-expression, mdb-thesis, transcription, translation, yeast},
	month = {January},
	number = {1},
	pages = {1--11},
	posted-at = {2009-01-29 20:07:41},
	priority = {1},
	title = {Comparative proteome analysis of Saccharomyces cerevisiae grown in chemostat cultures limited for glucose or ethanol.},
	url = {http://dx.doi.org/10.1074/mcp.M400087-MCP200},
